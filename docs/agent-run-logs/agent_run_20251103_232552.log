ata?: ParsedData\n    68â†’  error?: string\n    69â†’}\n    70â†’\n    71â†’/**\n    72â†’ * Helper function to safely extract text content from an XML element\n    73â†’ */\n    74â†’function getTextContent(element: Element | null, tagName: string, parentName: string = 'document'): string {\n    75â†’  if (!element) {\n    76â†’    throw new Error(`Missing required element: ${tagName} in ${parentName}`)\n    77â†’  }\n    78â†’  const node = element.querySelector(tagName)\n    79â†’  if (!node) {\n    80â†’    throw new Error(`Missing required element: ${tagName} in ${parentName}`)\n    81â†’  }\n    82â†’  return node.textContent?.trim() || ''\n    83â†’}\n    84â†’\n    85â†’/**\n    86â†’ * Parse address element (reusable for applicant and recipient addresses)\n    87â†’ */\n    88â†’function parseAddress(addressElement: Element | null, context: string): Address {\n    89â†’  if (!addressElement) {\n    90â†’    throw new Error(`Missing address element in ${context}`)\n    91â†’  }\n    92â†’\n    93â†’  return {\n    94â†’    street: getTextContent(addressElement, 'street', `${context} address`),\n    95â†’    city: getTextContent(addressElement, 'city', `${context} address`),\n    96â†’    state: getTextContent(addressElement, 'state', `${context} address`),\n    97â†’    zipCode: getTextContent(addressElement, 'zipCode', `${context} address`),\n    98â†’  }\n    99â†’}\n   100â†’\n   101â†’/**\n   102â†’ * Validate XML for well-formedness and basic schema requirements\n   103â†’ *\n   104â†’ * @param xmlString - XML string to validate\n   105â†’ * @returns ValidationResult with isValid flag and optional error message\n   106â†’ */\n   107â†’export function validateXml(xmlString: string): ValidationResult {\n   108â†’  if (!xmlString || xmlString.trim() === '') {\n   109â†’    return {\n   110â†’      isValid: false,\n   111â†’      error: 'XML input is empty. Please provide valid XML content.',\n   112â†’    }\n   113â†’  }\n   114â†’\n   115â†’  const parser = new DOMParser()\n   116â†’  const xmlDoc = parser.parseFromString(xmlString, 'text/xml')\n   117â†’\n   118â†’  // Check for parser errors\n   119â†’  const parserError = xmlDoc.querySelector('parsererror')\n   120â†’  if (parserError) {\n   121â†’    // Extract meaningful error message from parsererror element\n   122â†’    const errorText = parserError.textContent || 'Unknown XML parsing error'\n   123â†’\n   124â†’    // Provide user-friendly error messages for common issues\n   125â†’    if (errorText.includes('unclosed tag') || errorText.includes('Opening and ending tag mismatch')) {\n   126â†’      return {\n   127â†’        isValid: false,\n   128â†’        error: 'XML has unclosed or mismatched tags. Please check that all opening tags have corresponding closing tags.',\n   129â†’      }\n   130â†’    }\n   131â†’    if (errorText.includes('Unexpected end of file') || errorText.includes('premature end of data')) {\n   132â†’      return {\n   133â†’        isValid: false,\n   134â†’        error: 'XML is incomplete. The document appears to end unexpectedly.',\n   135â†’      }\n   136â†’    }\n   137â†’    if (errorText.includes('invalid character') || errorText.includes('not well-formed')) {\n   138â†’      return {\n   139â†’        isValid: false,\n   140â†’        error: 'XML contains invalid characters or syntax. Please check for special characters that need to be escaped.',\n   141â†’      }\n   142â†’    }\n   143â†’\n   144â†’    return {\n   145â†’      isValid: false,\n   146â†’      error: `XML parsing error: ${errorText}`,\n   147â†’    }\n   148â†’  }\n   149â†’\n   150â†’  // Check for root element\n   151â†’  const root = xmlDoc.documentElement\n   152â†’  if (!root || root.tagName !== 'applicationDocument') {\n   153â†’    return {\n   154â†’      isValid: false,\n   155â†’      error: 'Invalid XML structure. Root element must be <applicationDocument>.',\n   156â†’    }\n   157â†’  }\n   158â†’\n   159â†’  // Check for required top-level elements\n   160â†’  const requiredElements = ['applicant', 'date', 'recipient', 'letter']\n   161â†’  for (const elementName of requiredElements) {\n   162â†’    if (!root.querySelector(elementName)) {\n   163â†’      return {\n   164â†’        isValid: false,\n   165â†’        error: `Missing required element: <${elementName}>. All application documents must include applicant, date, recipient, and letter sections.`,\n   166â†’      }\n   167â†’    }\n   168â†’  }\n   169â†’\n   170â†’  return { isValid: true }\n   171â†’}\n   172â†’\n   173â†’/**\n   174â†’ * Parse XML string into structured data object\n   175â†’ *\n   176â†’ * @param xmlString - XML string to parse\n   177â†’ * @returns ParseResult with success flag, parsed data, or error message\n   178â†’ */\n   179â†’export function parseXml(xmlString: string): ParseResult {\n   180â†’  try {\n   181â†’    // First validate the XML\n   182â†’    const validation = validateXml(xmlString)\n   183â†’    if (!validation.isValid) {\n   184â†’      return {\n   185â†’        success: false,\n   186â†’        error: validation.error,\n   187â†’      }\n   188â†’    }\n   189â†’\n   190â†’    const parser = new DOMParser()\n   191â†’    const xmlDoc = parser.parseFromString(xmlString, 'text/xml')\n   192â†’    const root = xmlDoc.documentElement\n   193â†’\n   194â†’    // Extract formatStyle attribute (optional)\n   195â†’    const formatStyle = root.getAttribute('formatStyle') || undefined\n   196â†’\n   197â†’    // Parse applicant section\n   198â†’    const applicantElement = root.querySelector('applicant')\n   199â†’    if (!applicantElement) {\n   200â†’      throw new Error('Missing required element: applicant')\n   201â†’    }\n   202â†’\n   203â†’    const applicant: Applicant = {\n   204â†’      name: getTextContent(applicantElement, 'name', 'applicant'),\n   205â†’      address: parseAddress(applicantElement.querySelector('address'), 'applicant'),\n   206â†’      contactInformation: {\n   207â†’        phone: getTextContent(applicantElement, 'contactInformation phone', 'applicant'),\n   208â†’        email: getTextContent(applicantElement, 'contactInformation email', 'applicant'),\n   209â†’      },\n   210â†’    }\n   211â†’\n   212â†’    // Parse date\n   213â†’    const date = getTextContent(root, 'date')\n   214â†’\n   215â†’    // Parse recipient section\n   216â†’    const recipientElement = root.querySelector('recipient')\n   217â†’    if (!recipientElement) {\n   218â†’      throw new Error('Missing required element: recipient')\n   219â†’    }\n   220â†’\n   221â†’    const recipient: Recipient = {\n   222â†’      position: getTextContent(recipientElement, 'position', 'recipient'),\n   223â†’      company: getTextContent(recipientElement, 'company', 'recipient'),\n   224â†’      address: parseAddress(recipientElement.querySelector('address'), 'recipient'),\n   225â†’    }\n   226â†’\n   227â†’    // Parse letter section\n   228â†’    const letterElement = root.querySelector('letter')\n   229â†’    if (!letterElement) {\n   230â†’      throw new Error('Missing required element: letter')\n   231â†’    }\n   232â†’\n   233â†’    // Parse experiences\n   234â†’    const experienceElements = letterElement.querySelectorAll('experienceSection experience')\n   235â†’    const experiences: Experience[] = Array.from(experienceElements).map((expElement) => {\n   236â†’      const employer = expElement.querySelector('employer')?.textContent?.trim() || ''\n   237â†’      const achievementElements = expElement.querySelectorAll('achievements achievement')\n   238â†’      const achievements: Achievement[] = Array.from(achievementElements).map(ach => ({\n   239â†’        text: ach.textContent?.trim() || '',\n   240â†’      }))\n   241â†’\n   242â†’      return {\n   243â†’        employer,\n   244â†’        achievements,\n   245â†’      }\n   246â†’    })\n   247â†’\n   248â†’    const letter: Letter = {\n   249â†’      salutation: getTextContent(letterElement, 'salutation',